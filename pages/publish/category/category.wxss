/* pages/publish/category/category.wxss */
.division{
  height: 20rpx;
  background: #F2F3F4;
}
.cate-title{
  width:100%;
  display: flex;
  line-height:33rpx;
  padding:25rpx 0px;
  background: #fff;
  justify-content:center;
  border-bottom: 1px solid #f0f2f2;
  font-size:32rpx;
  align-items: center;
}
.cate-title image{
  width: 36rpx;
  height: 36rpx;
}
.cate-title text{
  padding: 0 40rpx;
}
.all-cate{
  width: 100%;
  background: #fff;
}
.cate{
  background: #fff;
  width: 100%;
  display: flex;
  font-size: 12px;
  color: #808080;
}
.cate .cate-detail{
  width: 20%;
  text-align: center;
  padding: 2vw 0;
}
.cate image{
  width: 80rpx;
  height: 80rpx;
}